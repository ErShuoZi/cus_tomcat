<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>Servlet</servlet-name>
        <servlet-class>com.Ls.servlet.Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Servlet</servlet-name>
        <url-pattern>/CalServlet</url-pattern>
    </servlet-mapping>

    <!--配置自定义的Servlet-->
    <servlet>
        <servlet-name>LsCalServlet</servlet-name>
        <servlet-class>com.Ls.tomcat.servlet.LsCalServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LsCalServlet</servlet-name>
        <url-pattern>/lsCalServlet</url-pattern>
    </servlet-mapping>
</web-app>